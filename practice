<Autocomplete
                    margin="normal"
                    fullWidth
                    size="small"
                    id="Department"
                    name="Department"
                    options={departmentpreviousList}
                    
                    // value={params.value || null}
                    value={departmentpreviousList.find(
                        (option) => option.valueId === formik.values.Department?.valueId
                    ) || null}
                    // getOptionLabel={(option) => option.label}
                    getOptionLabel={(value) => value.valueName}


                    onChange={(e, value) => {

                        handlefieldchange(params.row.id, "Department", value);
                    }}
                    sx={{ width: "100%", mt: 2 }}
                    renderInput={(params) => <TextField {...params} />}

                    
                    
                />


 onChange={(e, value) => {
                        console.log(value)
                        if (value === null) {
                            formik.setFieldValue("Department", null)
                        }
                        else
                            formik.setFieldValue("Department", value)
                    }}
