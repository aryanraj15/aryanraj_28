   <LocalizationProvider dateAdapter={AdapterDayjs}>
                    <DatePicker
                        label="From Date"
                        inputFormat="MM/DD/YYYY"
                        name="fromDate"
                        value={params.value}
                        onChange={(date) => {
                            
                            formik.setFieldValue("fromDate", dayjs(date).format("YYYY-MM-DD"))
                            console.log(dayjs(date).format("YYYY-MM-DD"))
                            setFromDate(dayjs(date).format("YYYY-MM-DD"))
                            // formik.setFieldValue("admissionDate", date);
                            handlefieldchange(params.row.id, "fromDate", dayjs(date).format("YYYY-MM-DD"));
                        }}

                        renderInput={(params) => (
                            <TextField
                                size="small"
                                fullWidth
                                sx={{ width: "100%", mt: 2 }}
                                {...params}
                                error={
                                    formik.touched.fromDate &&
                                    Boolean(formik.errors.fromDate)
                                }
                                helperText={
                                    formik.touched.fromDate &&
                                    formik.errors.fromDate
                                }
                            />
                        )}
                    />
                </LocalizationProvider>




<LocalizationProvider dateAdapter={AdapterDayjs}>
                    <DatePicker
                        label="To Date"
                        inputFormat="MM/DD/YYYY"
                        name="toDate"
                        value={params.value}
                        onChange={(date) => {

                            formik.setFieldValue("toDate", dayjs(date).format("YYYY-MM-DD"))
                            console.log(dayjs(date).format("YYYY-MM-DD"))
                            setToDate(dayjs(date).format("YYYY-MM-DD"))
                            // formik.setFieldValue("admissionDate", date);
                            handlefieldchange(params.row.id, "toDate", dayjs(date).format("YYYY-MM-DD"));
                        }}

                        renderInput={(params) => (
                            <TextField
                                size="small"
                                fullWidth
                                sx={{ width: "100%", mt: 2 }}
                                {...params}
                                error={
                                    formik.touched.toDate &&
                                    Boolean(formik.errors.toDate)
                                }
                                helperText={
                                    formik.touched.toDate &&
                                    formik.errors.toDate
                                }
                            />
                        )}
                    />
                </LocalizationProvider>
